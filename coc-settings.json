{
  "codeLens.enable": true,
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "suggest.completionItemKindLabels": {
    "method": "   Method ",
    "function": "   Function ",
    "variable": "[] Variable",
    "field": "  Field ",
    "typeParameter": "<>",
    "constant": "  ",
    "class": "  ",
    "interface": " ",
    "struct": "  ",
    "event": "  ",
    "operator": "  ",
    "module": " {} module ",
    "property": "  ",
    "enum": " ",
    "reference": "  ",
    "keyword": "  ",
    "file": "  ",
    "folder": " ﱮ ",
    "color": "  ",
    "unit": "  ",
    "snippet": " ✂ Snippet ",
    "text": "  ",
    "constructor": "  ",
    "value": "  ",
    "enumMember": "  "
  },
  "hover.target": "float",
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact"
  },
  "css.format.enable": true,
  "html.completion.attributeDefaultValue": "empty",
  "Lua.diagnostics.globals": ["vim"],
  "python.pythonPath": "python3",
  "snippets.ultisnips.pythonPrompt": false,
  "flutter.openDevLogSplitCommand": "botright 8split",
  "rust-analyzer.server.path": "rust-analyzer",
  "intelephense.licenceKey": "${env:INTELEPHENSE_LICENSE_KEY}",
  "cursors.cancelKey": "<C-c>",
  "languageserver": {
    "jsonnet": {
      "command": "jsonnet-language-server",
      "args": ["-t"],
      "rootPatterns": [".git/", "jsonnetfile.json"],
      "filetypes": ["jsonnet", "libsonnet"]
    }
  }
}
